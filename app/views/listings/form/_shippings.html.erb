<% i = 1%>
<%= form.fields_for :shippings, @shipping do |f|  %>
  <div class ="wrap_clone divider", id=  "<%="wrap#{i}" %>" >
	   <%= f.text_field :country, :placeholder => "country's name", class: "shipping-options-label" %>
	   <%= f.text_field :shipping_rate, class: "price-field delivery-price-field", maxlength: 10, placeholder: 0 %>
	   <% i = i + 1%>
   </div>
<% end %>
     